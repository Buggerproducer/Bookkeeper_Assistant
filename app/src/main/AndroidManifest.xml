<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybookkeeperassistant">

    <uses-permission android:name="android.permission.CAMERA" /> <!-- 读写SD卡权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".UniteApp"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon1"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyBookkeeperAssistant">
        <activity
            android:name=".MonthBill"
            android:exported="true" />
        <activity
            android:name=".UserInfo"
            android:exported="true" />
        <activity
            android:name=".Register"
            android:exported="true" />
        <activity
            android:name=".Login"
            android:exported="true" />
        <activity
            android:name=".Index"
            android:exported="true" />
        <activity
            android:name=".TotalDetail"
            android:exported="true" />
        <activity
            android:name=".ChartAnalysis"
            android:exported="true" />
        <activity
            android:name=".Setting"
            android:exported="true" />
        <activity
            android:name=".History"
            android:exported="true" />
        <activity
            android:name=".Search"
            android:exported="true" />
        <activity
            android:name=".Start"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Record"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>